<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="16dp"
        tools:context=".Activitys.MainActivity"
        tools:layout_editor_absoluteY="25dp" android:background="#8afff4c8"
>
    <android.support.v4.view.ViewPager
            android:id="@+id/viewPager"
            android:layout_width="match_parent"
            android:layout_height="220dp"
            android:background="@android:color/transparent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    />
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/songgolmae_aero"
            android:id="@+id/iv_newsFragmentAero"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/iv_newsFragmentMechanic"
            android:scaleType="fitXY" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/viewPager"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/songgolmae_mechanic"
            android:id="@+id/iv_newsFragmentMechanic"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentAero"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:scaleType="fitXY" app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentAero"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentAero" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@mipmap/ic_launcher"
            android:id="@+id/iv_newsFragmentManage"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentMaterial"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/iv_newsFragmentElec"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentAero"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.632"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@mipmap/ic_launcher"
            android:id="@+id/iv_newsFragmentElec"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentManage"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentManage" android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentManage"
            app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="98dp" app:srcCompat="@drawable/songgolmae_material"
            android:id="@+id/iv_newsFragmentMaterial"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/iv_newsFragmentManage"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentManage" android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentManage"
            app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/songolmae"
            android:id="@+id/iv_newsFragmentSoft"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/iv_newsFragmentAero" android:scaleType="fitXY"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentAero" android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentAero"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="항우기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentMechanic"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentMechanic"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentMechanic" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentMechanic" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/iv_newsFragmentElec"
            app:layout_constraintHorizontal_bias="0.527" app:layout_constraintVertical_bias="0.0"/>

    <TextView
            android:text="운항"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentAero"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentAero"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentAero" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentAero" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/iv_newsFragmentManage"
            app:layout_constraintVertical_bias="0.0"/>

    <TextView
            android:text="재료"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentMaterial"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentMaterial"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.272"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/tv_newsFragmentManage"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.336"/>

    <TextView
            android:text="경영"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentManage"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentManage"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentManage" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentManage" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"/>

    <TextView
            android:text="항전정"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentElec"
            android:fontFamily="@font/baedal"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentElec"
            app:layout_constraintStart_toEndOf="@+id/tv_newsFragmentManage" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.644" app:layout_constraintVertical_bias="0.272"/>

    <TextView
            android:text="소프트"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentSoft" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentSoft" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/iv_newsFragmentMaterial"
            app:layout_constraintHorizontal_bias="0.527" app:layout_constraintVertical_bias="0.21"
            android:fontFamily="@font/baedal"/>

</android.support.constraint.ConstraintLayout>
