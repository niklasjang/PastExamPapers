<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="16dp"
        tools:context=".Activitys.MainActivity"
        tools:layout_editor_absoluteY="25dp" android:background="#8afff4c8"
>
    <TextView
            android:text="자전"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentAutonomy"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentAutonomy"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentAutonomy"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentAutonomy"/>
    <TextView
            android:text="교물"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentTraffic"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentTraffic"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentTraffic"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentTraffic"/>
    <ImageView
            android:id="@+id/iv_newsFragmentAutonomy"
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentTraffic" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentTraffic"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentTraffic"
            app:layout_constraintVertical_bias="0.0"/>
    <android.support.v4.view.ViewPager
            android:id="@+id/viewPager"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:background="@android:color/transparent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="1.0"/>
    <ImageView
            android:layout_width="78dp"
            android:layout_height="78dp"
            app:srcCompat="@drawable/songgolmae_aero"
            android:id="@+id/iv_newsFragmentAero"
            android:scaleType="fitXY"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentMechanic"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentMechanic"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentMechanic" app:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            android:id="@+id/iv_newsFragmentMechanic"
            android:scaleType="fitXY"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentSoft"
            android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentSoft"/>
    <ImageView
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            android:id="@+id/iv_newsFragmentManage"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentSoft" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_newsFragmentSoft"/>
    <ImageView
            android:id="@+id/iv_newsFragmentElec"
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentManage" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentManage"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentManage"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:id="@+id/iv_newsFragmentTraffic"
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentElec"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentElec"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentElec" android:layout_marginStart="8dp"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:id="@+id/iv_newsFragmentMaterial"
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_material"
            app:layout_constraintTop_toTopOf="@+id/iv_newsFragmentAero"
            app:layout_constraintBottom_toBottomOf="@+id/iv_newsFragmentAero"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintStart_toEndOf="@+id/iv_newsFragmentAero"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="@+id/viewPager" android:layout_marginEnd="8dp"/>
    <ImageView
            android:id="@+id/iv_newsFragmentSoft"
            android:layout_width="78dp"
            android:layout_height="78dp" app:srcCompat="@drawable/songgolmae_mechanic"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="@+id/viewPager" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/viewPager"/>

    <TextView
            android:text="항우기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentMechanic"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentMechanic"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentMechanic"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentMechanic"/>


    <TextView
            android:text="운항"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentAero"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentAero"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentAero"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentAero"/>

    <TextView
            android:text="재료"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentMaterial"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentMaterial"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentMaterial"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentMaterial"/>

    <TextView
            android:text="경영"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentManage"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentManage"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentManage"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentManage"/>

    <TextView
            android:text="항전정"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentElec"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentElec"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentElec"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentElec" app:layout_constraintHorizontal_bias="0.522"/>

    <TextView
            android:text="소프트"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_newsFragmentSoft"
            android:fontFamily="@font/baedal"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintStart_toStartOf="@+id/iv_newsFragmentSoft"
            app:layout_constraintEnd_toEndOf="@+id/iv_newsFragmentSoft"/>

</android.support.constraint.ConstraintLayout>
